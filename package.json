{
  "name": "lc-ts-lodash",
  "version": "0.6.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/lindellcarternyc/ts-lodash.git"
  },
  "description": "MY personal ts version of lodash",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "watch:tests": "jest --config jestconfig.json --watchAll",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Typescript",
    "Lodash",
    "Utils"
  ],
  "author": "Lindell Carter <lindellcarternyc@gmail.com> (https://github.com/lindellcarternyc)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.118",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  }
}
